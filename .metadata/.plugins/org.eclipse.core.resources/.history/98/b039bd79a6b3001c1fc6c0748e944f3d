package programando1.classes;

public class Matriz {
	
	/*Executar codigo java*/
	public static void main(String[] args) {
		
		
		int notas [][] = new int [1][3];
		
		notas [0][0] = 80;
		notas [0][1] = 90;
		notas [0][2] = 70;
		
		notas [1][0] = 40;
		notas [1][1] = 60;
		notas [1][2] = 30;
		
		System.out.println(notas);
		
		
	}

}
